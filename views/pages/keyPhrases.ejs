<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head', {title: 'Key Phrases'}); %>
<body>
  <%- include('../partials/header'); %>
  <h1 class="keyword-header">Key Phrases:</h1>
  <main>
      <ul class="keyword-list">
        <% phrases.forEach( phrase => {  %>
          <li class="keyword"><%= phrase %></li>
        <% }) %>
      </ul>
  </main>
  <a href="/patient/<%= patient_id %>">Back</a>
  <%- include('../partials/footer'); %>
</body>
</html>